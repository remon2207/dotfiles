let skip_defaults_vim = 1

set nocompatible
set autoindent
set autoread
set clipboard^=unnamed
set cursorline
set expandtab
set encoding=utf-8
set fileencoding=utf-8
set fileformat=unix
set helplang=ja,en
set laststatus=2
set number
set showcmd
set smartindent
set smarttab
set softtabstop=4
set noswapfile
set tabstop=4
set wildmenu
set ttimeout
set ttimeoutlen=100
set display=truncate
set incsearch

syntax on
filetype plugin indent on
